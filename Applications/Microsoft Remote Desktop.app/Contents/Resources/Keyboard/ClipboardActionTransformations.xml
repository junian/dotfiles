                <to control="1" key="D" />
            </transform>

            <!-- Command+E to Control+E -->
            <transform>
                <from command="1" key="E" />
                <to control="1" key="E" />
            </transform>

            <!-- Command+F to Control+F -->
            <transform>
                <from command="1" key="F" />
                <to control="1" key="F" />
            </transform>

